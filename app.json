{
  "expo": {
    "name": "Ambi",
    "slug": "ambi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Ambi's goal is to help its users become ambidextrous! The app provides habit tracking for daily non-dominant hand activities, hand-strengthening tap games, and guided writing and sports exercises. ",
    "githubUrl": "https://github.com/dakotagoldberg/ambi"
  }
}
